{
  "manifest_version": 2,
  "version": "1.0",
  "short_name": "tfpTestExt",
  "name": "tfpractice test extension",
  "icons": {
    "16": "favicon.ico",
    "24": "favicon.ico",
    "32": "favicon.ico",
    "64": "favicon.ico"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "permissions": ["activeTab"],
    "start_url": "./index.html",
    "display": "standalone",
    "theme_color": "#000000",
    "background_color": "#ffffff"
  }
}
